<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>


<BorderPane prefHeight="640.0" prefWidth="1024.0"
    xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx/15" fx:controller="org.openjfx.FXMLController">
 
       <top>
          <Label fx:id="label" text="Welcome to Family tree application" />
          
          <HBox spacing="20">
              <Button fx:id="button1" text="Load Tree" />
              <Button fx:id="button2" text="Save Tree" />
              <Button fx:id="button3" text="Create Tree" />
          </HBox>

       </top>
       
       <center>
            <HBox>
                <TreeView fx:id="locationTreeView" layoutX="0" layoutY="0" prefHeight="193.0" prefWidth="471.0" />
                <Rectangle fx:id="rectangle" fill="GREY" />
            </HBox>
       </center>
 
</BorderPane>

